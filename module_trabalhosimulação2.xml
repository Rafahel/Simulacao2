<?xml version="1.0" encoding="UTF-8"?>
<project name="module_trabalhosimulação2" default="compile.module.trabalhosimulação2">
  <dirname property="module.trabalhosimulação2.basedir" file="${ant.file.module_trabalhosimulação2}"/>
  
  <property name="module.jdk.home.trabalhosimulação2" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.trabalhosimulação2" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.trabalhosimulação2" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.trabalhosimulação2" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="trabalhosimulação2.output.dir" value="${module.trabalhosimulação2.basedir}/out/production/TrabalhoSimulação2"/>
  <property name="trabalhosimulação2.testoutput.dir" value="${module.trabalhosimulação2.basedir}/out/test/TrabalhoSimulação2"/>
  
  <path id="trabalhosimulação2.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="trabalhosimulação2.module.production.classpath">
    <path refid="${module.jdk.classpath.trabalhosimulação2}"/>
  </path>
  
  <path id="trabalhosimulação2.runtime.production.module.classpath">
    <pathelement location="${trabalhosimulação2.output.dir}"/>
  </path>
  
  <path id="trabalhosimulação2.module.classpath">
    <path refid="${module.jdk.classpath.trabalhosimulação2}"/>
    <pathelement location="${trabalhosimulação2.output.dir}"/>
  </path>
  
  <path id="trabalhosimulação2.runtime.module.classpath">
    <pathelement location="${trabalhosimulação2.testoutput.dir}"/>
    <pathelement location="${trabalhosimulação2.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.trabalhosimulação2">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.trabalhosimulação2">
    <patternset refid="excluded.from.module.trabalhosimulação2"/>
  </patternset>
  
  <path id="trabalhosimulação2.module.sourcepath">
    <dirset dir="${module.trabalhosimulação2.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.trabalhosimulação2" depends="compile.module.trabalhosimulação2.production,compile.module.trabalhosimulação2.tests" description="Compile module TrabalhoSimulação2"/>
  
  <target name="compile.module.trabalhosimulação2.production" depends="register.custom.compilers" description="Compile module TrabalhoSimulação2; production classes">
    <mkdir dir="${trabalhosimulação2.output.dir}"/>
    <javac2 destdir="${trabalhosimulação2.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.trabalhosimulação2}/javac">
      <compilerarg line="${compiler.args.trabalhosimulação2}"/>
      <bootclasspath refid="trabalhosimulação2.module.bootclasspath"/>
      <classpath refid="trabalhosimulação2.module.production.classpath"/>
      <src refid="trabalhosimulação2.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.trabalhosimulação2"/>
    </javac2>
    
    <copy todir="${trabalhosimulação2.output.dir}">
      <fileset dir="${module.trabalhosimulação2.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.trabalhosimulação2.tests" depends="register.custom.compilers,compile.module.trabalhosimulação2.production" description="compile module TrabalhoSimulação2; test classes" unless="skip.tests"/>
  
  <target name="clean.module.trabalhosimulação2" description="cleanup module">
    <delete dir="${trabalhosimulação2.output.dir}"/>
    <delete dir="${trabalhosimulação2.testoutput.dir}"/>
  </target>
</project>